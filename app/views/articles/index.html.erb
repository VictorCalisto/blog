<p style="color: green"><%= notice %></p>

<h1>Articles</h1>
<%= render "nav" %>
<div id="articles">
  <% @articles.each do |article| %>
    <%= render article %>
    <p>
      <%= link_to "Show this article", article %>
    </p>
  <% end %>
</div>

<%= link_to "New article", new_article_path,class:"btn btn-primary" %>
